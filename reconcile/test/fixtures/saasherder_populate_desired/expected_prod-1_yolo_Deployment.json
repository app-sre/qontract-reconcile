{
  "apiVersion":"apps/v1",
  "kind":"Deployment",
  "metadata":{
    "labels":{
      "app.kubernetes.io/version":"v100.1.1"
    },
    "name":"some-app"
  },
  "spec":{
    "replicas":5,
    "selector":{
      "matchLabels":{
        "app.kubernetes.io/name":"some-app"
      }
    },
    "strategy":{
      "rollingUpdate":{
        "maxSurge":0,
        "maxUnavailable":1
      }
    },
    "template":{
      "metadata":{
        "labels":{
          "app.kubernetes.io/name":"some-app",
          "app.kubernetes.io/version":"v100.1.1"
        }
      },
      "spec":{
        "containers":[
          {
            "args":[
              "--different_metrics.read.endpoint=http://observatorium-thanos-query-frontend.yolo.svc.cluster.local:9090",
              "--different_metrics.write.endpoint=http://observatorium-thanos-receive.yolo.svc.cluster.local:19291",
              "--different_metrics.read.endpoint=http://observatorium-loki-query-frontend-http.yolo-but-for-logs.svc.cluster.local:3100",
              "--different_metrics.tail.endpoint=http://observatorium-loki-querier-http.yolo-but-for-logs.svc.cluster.local:3100",
              "--different_metrics.write.endpoint=http://observatorium-loki-distributor-http.yolo-but-for-logs.svc.cluster.local:3100",
              "--different_metrics.rate-limiter.grpc-address=observatorium-gubernator.yolo.svc.cluster.local:8081",
              "--different_metrics.concurrent-request-limit=50"
            ],
            "image":"quay.io/observatorium/api:v100.1.1",
            "name":"observatorium-api",
            "resources":{
              "limits":{
                "cpu":"1",
                "memory":"1Gi"
              },
              "requests":{
                "cpu":"100m",
                "memory":"256Mi"
              }
            }
          }
        ],
        "serviceAccountName":"maor-for-president"
      }
    }
  }
}
